{
  "name": "compat-jest26",
  "version": "0.9.0",
  "description": "Package to test @propcheck/jest against jest 26.x",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "echo 'nothing to do'",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "setupFilesAfterEnv": [
      "@propcheck/jest"
    ],
    "reporters": [
      "jest-standard-reporter"
    ]
  },
  "devDependencies": {
    "@propcheck/core": "0.10.0",
    "@propcheck/jest": "0.10.0",
    "@types/jest": "26.0.14",
    "jest": "26.6.3",
    "ts-jest": "26.5.4",
    "typescript": "4.4.3"
  }
}
