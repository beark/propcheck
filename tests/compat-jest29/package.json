{
    "name": "compat-jest29",
    "version": "0.9.0",
    "description": "Package to test @propcheck/jest against jest 29.x",
    "license": "MIT",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "lint": "echo 'nothing to do'",
        "test": "jest"
    },
    "jest": {
        "preset": "ts-jest",
        "watchPathIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": [
                "ts-jest",
                {
                    "tsconfig": "tsconfig.json"
                }
            ]
        },
        "setupFilesAfterEnv": [
            "@propcheck/jest"
        ],
        "reporters": [
            "jest-standard-reporter"
        ]
    },
    "devDependencies": {
        "@propcheck/core": "workspace:*",
        "@propcheck/jest": "workspace:*",
        "@types/jest": "29.2.1",
        "jest": "29.2.2",
        "ts-jest": "29.0.3",
        "typescript": "4.8.4"
    }
}
