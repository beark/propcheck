{
    "name": "@propcheck/core",
    "version": "0.12.0",
    "description": "Core library of propcheck, a property based testing suite",
    "license": "MIT",
    "main": "index.js",
    "types": "index.d.ts",
    "files": [
        "CHANGELOG.md",
        "generators/*.js",
        "generators/*.d.ts",
        "Gen.d.ts",
        "Gen.js",
        "index.d.ts",
        "index.js",
        "prng.d.ts",
        "prng.js",
        "Range.d.ts",
        "Range.js",
        "runner.d.ts",
        "runner.js",
        "shrink.d.ts",
        "shrink.js",
        "Tree.d.ts",
        "Tree.js",
        "type-utils.d.ts"
    ],
    "engines": {
        "node": ">=12.17.0 <17"
    },
    "keywords": [
        "functional",
        "propcheck",
        "property-based testing",
        "quickcheck",
        "test",
        "testing",
        "typescript"
    ],
    "author": "BjÃ¶rn Aili <4073478+beark@users.noreply.github.com> (https://github.com/beark)",
    "repository": "github:beark/propcheck",
    "homepage": "https://github.com/beark/propcheck",
    "bugs": "https://github.com/beark/propcheck/issues",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "clean": "git clean -fx -- *.js *.d.ts *.d.ts.map *.log && rm -rf generators/ coverage/",
        "lint": "eslint \"./src/**/*\"",
        "test": "jest"
    },
    "jest": {
        "preset": "ts-jest",
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "watchPathIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "globals": {
            "ts-jest": {
                "packageJson": "package.json",
                "tsConfig": "tsconfig.json"
            }
        },
        "reporters": [
            "jest-standard-reporter"
        ]
    },
    "dependencies": {
        "lazy-sequences": "^2.0.0",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@types/jest": "26.0.14",
        "@typescript-eslint/eslint-plugin": "4.29.1",
        "@typescript-eslint/parser": "4.29.1",
        "eslint": "7.32.0",
        "jest": "27.0.6",
        "jest-standard-reporter": "2.0.0",
        "prettier": "2.3.2",
        "ts-jest": "27.0.4",
        "typescript": "4.4.3"
    }
}
