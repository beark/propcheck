{
    "name": "@propcheck/jest",
    "version": "0.10.2",
    "description": "Jest integration/plugin for propcheck",
    "license": "MIT",
    "main": "index.js",
    "types": "index.d.ts",
    "files": [
        "CHANGELOG.md",
        "index.js",
        "index.d.ts"
    ],
    "engines": {
        "node": ">=12.17.0 <20"
    },
    "keywords": [
        "functional",
        "jest",
        "propcheck",
        "property-based testing",
        "quickcheck",
        "test",
        "testing",
        "typescript"
    ],
    "author": "BjÃ¶rn Aili <4073478+beark@users.noreply.github.com> (https://github.com/beark)",
    "repository": "github:beark/propcheck",
    "homepage": "https://github.com/beark/propcheck",
    "bugs": "https://github.com/beark/propcheck/issues",
    "sideEffects": [
        "index.js"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "clean": "git clean -fx -- *.js *.d.ts *.d.ts.map *.log && rm -rf coverage/",
        "lint": "eslint \"./src/**/*\"",
        "test": "node ../../common/scripts/runForNodeGeq 14.0.0 jest"
    },
    "jest": {
        "preset": "ts-jest",
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "watchPathIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": [
                "ts-jest",
                {
                    "packageJson": "package.json",
                    "tsconfig": "tsconfig.json"
                }
            ]
        },
        "reporters": [
            "jest-standard-reporter"
        ]
    },
    "dependencies": {
        "@propcheck/core": "^0.12.1",
        "tslib": "^2.4.1"
    },
    "peerDependencies": {
        "@types/jest": ">= 24.0.20 < 30",
        "jest": ">= 24.1.0 < 30"
    },
    "devDependencies": {
        "@types/jest": "29.2.1",
        "@types/node": "18.11.9",
        "@typescript-eslint/eslint-plugin": "5.42.0",
        "@typescript-eslint/parser": "5.42.0",
        "eslint": "8.26.0",
        "jest": "29.2.2",
        "jest-standard-reporter": "2.0.0",
        "prettier": "2.7.1",
        "ts-jest": "29.0.3",
        "typescript": "4.8.4"
    }
}
