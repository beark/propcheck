{
    "name": "@propcheck/jest",
    "version": "0.10.0",
    "description": "Jest integration/plugin for propcheck",
    "license": "MIT",
    "main": "index.js",
    "types": "index.d.ts",
    "files": [
        "CHANGELOG.md",
        "index.js",
        "index.d.ts"
    ],
    "engines": {
        "node": ">=12.17.0 <17"
    },
    "keywords": [
        "functional",
        "jest",
        "propcheck",
        "property-based testing",
        "quickcheck",
        "test",
        "testing",
        "typescript"
    ],
    "author": "BjÃ¶rn Aili <4073478+beark@users.noreply.github.com> (https://github.com/beark)",
    "repository": "github:beark/propcheck",
    "homepage": "https://github.com/beark/propcheck",
    "bugs": "https://github.com/beark/propcheck/issues",
    "sideEffects": [
        "index.js"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "clean": "git clean -fx -- *.js *.d.ts *.d.ts.map *.log && rm -rf coverage/",
        "lint": "eslint \"./src/**/*\"",
        "test": "jest"
    },
    "jest": {
        "preset": "ts-jest",
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "watchPathIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "globals": {
            "ts-jest": {
                "packageJson": "package.json",
                "tsConfig": "tsconfig.json"
            }
        },
        "reporters": [
            "jest-standard-reporter"
        ]
    },
    "dependencies": {
        "@propcheck/core": "^0.10.0",
        "tslib": "^2.3.0"
    },
    "peerDependencies": {
        "@types/jest": ">= 24.0.20 < 28",
        "jest": ">= 24.1.0 < 28"
    },
    "devDependencies": {
        "@types/jest": "26.0.14",
        "@types/node": "12.12.2",
        "@typescript-eslint/eslint-plugin": "4.29.1",
        "@typescript-eslint/parser": "4.29.1",
        "eslint": "7.32.0",
        "jest": "27.0.6",
        "jest-standard-reporter": "2.0.0",
        "prettier": "2.3.2",
        "ts-jest": "27.0.4",
        "typescript": "4.3.5"
    }
}
